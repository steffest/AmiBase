<configuration>  
  <system.webServer>  
    <webSocket enabled="false" />  
    <handlers>  
      <add name="iisnode" path="server.js" verb="*" modules="iisnode" />  
    </handlers>  
    
    <rewrite>
           <rules>
             <rule name="hello">
               <match url="^$" />
